{
  "name": "webpack-toy",
  "version": "1.0.0",
  "description": "Webpack and Babel with CSS Modules, PostCSS, and Karma testing.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "test": "karma start --single-run",
    "jest": "node node_modules/.bin/jest",
    "jest-update": "node node_modules/.bin/jest --updateSnapshot",
    "lint": "eslint src/**/*.js",
    "build": "webpack --mode=production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0",
    "@mavenlink/design-system": "^0.7.0",
    "babel-loader": "^8.0.0-beta.3",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "testRegex": "./lib/jest/.*-spec.js(x)?$",
    "rootDir": ".",
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/syntax-dynamic-import"
    ]
  },
  "optionalDependencies": {
    "mini-css-extract-plugin": "^0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jasmine-core": "^3.4.0",
    "jest": "^24.7.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.5",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2"
  }
}
